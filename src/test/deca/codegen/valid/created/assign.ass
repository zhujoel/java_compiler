; start main program
; Main program
; On stocke la valeur "null" à la première case de la pile.
	LOAD #null, R0
	STORE R0, 1(GB)
; Beginning of main instructions:
; Test pour savoir si la pile est pleine
	TSTO #3
	BOV pile_pleine
	ADDSP #3
; Declaration des variables
	LOAD #0, R2
	STORE R2, 2(GB)
; Instructions
Begin_While_1:
; >
	LOAD 2(GB), R2
	LOAD #10, R3
	CMP R3, R2
	BLE End_While_1
In_While_1:
; +
	LOAD 2(GB), R3
	LOAD #1, R4
	ADD R3, R4
	BOV debordement
	STORE R4, 2(GB)
	LOAD 2(GB), R1
	WINT
	WNL
	BRA Begin_While_1
End_While_1:
	WSTR """fin du while"""
	WNL
	HALT
pile_pleine:
	WSTR "Erreur : Pile pleine."
	WNL
	ERROR
dereferencement_null:
	WSTR "Erreur : dereferencement null."
	WNL
	ERROR
tas_plein:
	WSTR "Erreur : Tas plein."
	WNL
	ERROR
debordement:
	WSTR "Erreur : debordement durant une operation arithmétique"
	WNL
	ERROR
io_error:
	WSTR "Erreur : input ou output"
	WNL
	ERROR
; end main program
