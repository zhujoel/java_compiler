; Test pour savoir si la pile est pleine
	TSTO #22
	BOV pile_pleine
	ADDSP #20
; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 16(GB)
	LOAD code.Object.equals, R0
	STORE R0, 17(GB)
; Main Function
; Declaration des variables
	LOAD #0x1.266666p1, R2
	STORE R2, 18(GB)
	LOAD #0x1.333334p0, R2
	STORE R2, 19(GB)
; Beginning of main instructions:
; *
	LOAD #0x1.333334p0, R2
	LOAD #0x1.4p1, R3
	MUL R3, R2
	BOV debordement
	STORE R2, 18(GB)
	LOAD 18(GB), R1
	WFLOAT
	HALT
code.Object.equals:
