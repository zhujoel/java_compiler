; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 4(GB)
	LOAD code.Object.equals, R0
	STORE R0, 5(GB)
code.Object.equals:
; Main Function
; Beginning of main instructions:
; Test pour savoir si la pile est pleine
	TSTO #4
	BOV pile_pleine
	ADDSP #4
; Declaration des variables
	LOAD #5, R2
	STORE R2, 11(GB)
; -
	LOAD #4, R2
	SUB #0, R2
	STORE R2, 12(GB)
; Instructions
; +
	LOAD 12(GB), R2
	LOAD 11(GB), R3
	ADD R2, R3
	BOV debordement
	STORE R3, 11(GB)
	LOAD 11(GB), R1
	WINT
	HALT
pile_pleine:
	WSTR "Erreur : Pile pleine."
	WNL
	ERROR
dereferencement_null:
	WSTR "Erreur : dereferencement null."
	WNL
	ERROR
tas_plein:
	WSTR "Erreur : Tas plein."
	WNL
	ERROR
debordement:
	WSTR "Erreur : debordement durant une operation arithmétique"
	WNL
	ERROR
io_error:
	WSTR "Erreur : input ou output"
	WNL
	ERROR
; end main program
