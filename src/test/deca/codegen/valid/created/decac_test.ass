; start main program
; Main program
; On stocke la valeur "null" à la première case de la pile.
	LOAD #null, R0
	STORE R0, 1(GB)
; Beginning of main instructions:
; Test pour savoir si la pile est pleine
	TSTO #3
	BOV pile_pleine
	ADDSP #3
; Declaration des variables
	LOAD #0, R2
	STORE R2, 2(GB)
; Instructions
; Début du if_1
; ||
; &&
; &&
; &&
	LOAD 2(GB), R2
	CMP #1, R2
	BNE Fin_or_1
; &&
	BRA Fin_or_1
Fin_or_1:
; Contenu du if_1
	WSTR "dans le if"
	WNL
	BRA endif_1
else_1:
	WSTR "dans le else"
	WNL
endif_1:
	WSTR "fin"
	WNL
	HALT
pile_pleine:
	WSTR "Erreur : Pile pleine."
	WNL
	ERROR
dereferencement_null:
	WSTR "Erreur : dereferencement null."
	WNL
	ERROR
tas_plein:
	WSTR "Erreur : Tas plein."
	WNL
	ERROR
debordement:
	WSTR "Erreur : debordement durant une operation arithmétique"
	WNL
	ERROR
io_error:
	WSTR "Erreur : input ou output"
	WNL
	ERROR
; end main program
