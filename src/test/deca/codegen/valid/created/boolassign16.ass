; Test pour savoir si la pile est pleine
	TSTO #22
	BOV pile_pleine
	ADDSP #20
; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 17(GB)
	LOAD code.Object.equals, R0
	STORE R0, 18(GB)
; Main Function
; Declaration des variables
; >=
	LOAD #0, R2
	LOAD #1, R3
	CMP R3, R2
	STORE R2, 19(GB)
; Beginning of main instructions:
; ||
; &&
; !
	LOAD 19(GB), R2
	SUB #1, R2
	LOAD #1, R3
	MUL R3, R2
; &&
	LOAD #0, R3
	LOAD #1, R4
	MUL R4, R3
	ADD R3, R2
	SHR R2
	STORE R2, 19(GB)
	HALT
code.Object.equals:
pile_pleine:
	WSTR "Erreur : Pile pleine."
	WNL
	ERROR
dereferencement_null:
