; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 12(GB)
	LOAD code.Object.equals, R0
	STORE R0, 13(GB)
; Main Function
; Test pour savoir si la pile est pleine
	TSTO #4
	BOV pile_pleine
	ADDSP #4
; Declaration des variables
	LOAD #3, R2
	STORE R2, 14(GB)
	LOAD #2, R2
	STORE R2, 15(GB)
; Beginning of main instructions:
; *
	LOAD 14(GB), R2
	LOAD 15(GB), R3
	MUL R2, R3
	BOV debordement
	STORE R3, 14(GB)
	LOAD 14(GB), R1
	WINT
	WNL
	HALT
pile_pleine:
	WSTR "Erreur : Pile pleine."
	WNL
	ERROR
dereferencement_null:
