; Test pour savoir si la pile est pleine
	TSTO #7
	BOV pile_pleine
	ADDSP #5
; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Main Function
; Declaration des variables
	RINT
	BOV io_error
	STORE R1, 3(GB)
	LOAD #0, R0
	STORE R0, 4(GB)
; Beginning of main instructions:
	LOAD 3(GB), R1
	WINT
	RINT
	BOV io_error
	STORE R1, 4(GB)
	LOAD 4(GB), R1
	WINT
	HALT
code.Object.equals:
pile_pleine:
	WSTR "Erreur : Pile pleine."
	WNL
	ERROR
dereferencement_null:
	WSTR "Erreur : dereferencement null."
	WNL
	ERROR
tas_plein:
	WSTR "Erreur : Tas plein."
	WNL
	ERROR
debordement:
	WSTR "Erreur : debordement durant une operation arithmétique"
	WNL
	ERROR
io_error:
	WSTR "Erreur : input ou output"
	WNL
	ERROR
; end main program
