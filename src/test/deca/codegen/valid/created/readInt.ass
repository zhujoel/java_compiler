; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 5(GB)
	LOAD code.Object.equals, R0
	STORE R0, 7(GB)
; Main Function
; Test pour savoir si la pile est pleine
	TSTO #4
	BOV pile_pleine
	ADDSP #4
; Declaration des variables
	RINT
	BOV io_error
	STORE R1, 13(GB)
	LOAD #0, R0
	STORE R0, 15(GB)
; Beginning of main instructions:
	LOAD 13(GB), R1
	WINT
	RINT
	BOV io_error
	STORE R1, 15(GB)
	LOAD 15(GB), R1
	WINT
	HALT
pile_pleine:
	WSTR "Erreur : Pile pleine."
	WNL
	ERROR
dereferencement_null:
	WSTR "Erreur : dereferencement null."
	WNL
	ERROR
tas_plein:
	WSTR "Erreur : Tas plein."
