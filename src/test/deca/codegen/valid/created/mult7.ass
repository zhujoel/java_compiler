; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
code.Object.equals:
; Main Function
; Beginning of main instructions:
; Test pour savoir si la pile est pleine
	TSTO #4
	BOV pile_pleine
	ADDSP #4
; Declaration des variables
	LOAD #5, R2
	STORE R2, 3(GB)
	LOAD #4, R2
	STORE R2, 4(GB)
; Instructions
; *
	LOAD 4(GB), R2
	PUSH R2
	LOAD 3(GB), R2
	MUL R2, R2
	BOV debordement
	POP R2
	STORE R2, 3(GB)
	LOAD 3(GB), R1
	WINT
	HALT
pile_pleine:
	WSTR "Erreur : Pile pleine."
	WNL
	ERROR
dereferencement_null:
	WSTR "Erreur : dereferencement null."
	WNL
	ERROR
tas_plein:
	WSTR "Erreur : Tas plein."
	WNL
	ERROR
debordement:
	WSTR "Erreur : debordement durant une operation arithmétique"
	WNL
	ERROR
io_error:
	WSTR "Erreur : input ou output"
	WNL
	ERROR
; end main program
