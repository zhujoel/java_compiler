; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 7(GB)
	LOAD code.Object.equals, R0
	STORE R0, 8(GB)
; Main Function
; Test pour savoir si la pile est pleine
	TSTO #3
	BOV pile_pleine
	ADDSP #3
; Declaration des variables
	LOAD #1, R2
	STORE R2, 9(GB)
; Beginning of main instructions:
; Début du if_1
	LOAD 9(GB), R2
	CMP #1, R2
	BNE else_1
; Contenu du if_1
	WSTR "Dans le if"
	WNL
	BRA endif_1
else_1:
	WSTR "Dans le else"
	WNL
endif_1:
	HALT
pile_pleine:
	WSTR "Erreur : Pile pleine."
	WNL
	ERROR
dereferencement_null:
