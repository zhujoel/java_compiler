; Test pour savoir si la pile est pleine
	TSTO #7
	BOV pile_pleine
	ADDSP #5
; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Main Function
; Declaration des variables
	LOAD #5, R2
	STORE R2, 3(GB)
	LOAD #5, R2
	STORE R2, 4(GB)
; Beginning of main instructions:
; Début du if_1
; >
	LOAD 3(GB), R2
	LOAD 4(GB), R3
	CMP R3, R2
	BLE else_1
; Contenu du if_1
	WSTR "dans le if"
	BRA endif_1
else_1:
	WSTR "dans le else"
endif_1:
	HALT
code.Object.equals:
pile_pleine:
	WSTR "Erreur : Pile pleine."
