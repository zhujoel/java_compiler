; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 5(GB)
	LOAD code.Object.equals, R0
	STORE R0, 6(GB)
; Main Function
; Test pour savoir si la pile est pleine
	TSTO #4
	BOV pile_pleine
	ADDSP #4
; Declaration des variables
; -
	LOAD #5, R2
	SUB #0, R2
	STORE R2, 9(GB)
	LOAD #4, R2
	STORE R2, 10(GB)
; Beginning of main instructions:
; *
	LOAD 10(GB), R2
	LOAD 9(GB), R3
	MUL R2, R3
	BOV debordement
	STORE R3, 9(GB)
	LOAD 9(GB), R1
	WINT
	HALT
pile_pleine:
	WSTR "Erreur : Pile pleine."
	WNL
	ERROR
dereferencement_null:
