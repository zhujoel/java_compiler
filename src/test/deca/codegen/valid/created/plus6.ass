; start main program
; Class Declaration
; Code de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 17(GB)
	LOAD code.Object.equals, R0
	STORE R0, 18(GB)
; Main Function
; Test pour savoir si la pile est pleine
	TSTO #4
	BOV pile_pleine
	ADDSP #4
; Declaration des variables
	LOAD #5, R2
	STORE R2, 19(GB)
	LOAD #4, R2
	STORE R2, 20(GB)
; Beginning of main instructions:
; +
	LOAD 19(GB), R2
	PUSH R2
	LOAD 20(GB), R2
	ADD R2, R2
	BOV debordement
	POP R2
	STORE R2, 19(GB)
	LOAD 19(GB), R1
	WINT
	HALT
