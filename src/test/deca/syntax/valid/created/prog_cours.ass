; start main program
; Main program
; On stocke la valeur "null" à la première case de la pile.
	LOAD #null, R0
	STORE R0, 1(GB)
; Beginning of main instructions:
	LOAD #0, R0
	STORE R0, 2(GB)
	ADDSP #1
	LOAD #1, R0
	STORE R0, 3(GB)
	ADDSP #1
	LOAD #1, R0
	STORE R0, 4(GB)
	ADDSP #1
; &&
	LOAD 2(GB), R0
	LOAD 3(GB), R1
	MUL R0, R1
	STORE R0, 2(GB)
; ||
	LOAD 3(GB), R1
	LOAD 2(GB), R2
	ADD R1, R2
	SHR R1
	STORE R1, 3(GB)
	LOAD 4(GB), R1
	WINT
	LOAD 3(GB), R1
	WINT
	HALT
; end main program
